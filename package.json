{
  "name": "vs-rainbow-folders",
  "publisher": "BenWilles",
  "displayName": "VS Rainbow Folders",
  "description": "Colorize folders in the VS Code file explorer with a curated pastel color palette",
  "version": "1.0.0",
  "repository": "https://github.com/BenWilles/vs-rainbow-folders",
  "icon": "./icon.png",
  "author": {
    "name": "Ben Willes"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "vs-rainbow-folders.setColor"
        }
      ]
    },
    "commands": [
      {
        "command": "vs-rainbow-folders.setColor",
        "title": "Colorize"
      }
    ],
    "configuration": {
      "title": "VS Rainbow Folders",
      "properties": {
        "vs-rainbow-folders.pathColors": {
          "type": "array",
          "default": [],
          "description": "Array of folder paths and their colors",
          "items": {
            "type": "object",
            "properties": {
              "folderPath": {
                "type": "string",
                "description": "The folder path"
              },
              "color": {
                "type": "string",
                "description": "The color ID"
              }
            }
          }
        }
      }
    },
    "colors": [
      {
        "id": "foldercolorizer.color_ff9999",
        "defaults": {
          "dark": "#ff9999",
          "light": "#ff9999",
          "highContrast": "#ff9999",
          "highContrastLight": "#ff9999"
        },
        "description": "Light Salmon"
